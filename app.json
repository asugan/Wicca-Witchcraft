{
  "expo": {
    "name": "Wicca Witchcraft & Spells",
    "slug": "wicca-witchcraft",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "wiccawitchcraft",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "owner": "asugan",
    "locales": {
      "en-US": "./locales/native/en-US.json",
      "en-GB": "./locales/native/en-GB.json",
      "tr": "./locales/native/tr.json",
      "de-DE": "./locales/native/de-DE.json",
      "es-ES": "./locales/native/es-ES.json",
      "es-MX": "./locales/native/es-MX.json",
      "fr-FR": "./locales/native/fr-FR.json",
      "fr-CA": "./locales/native/fr-CA.json",
      "it": "./locales/native/it.json",
      "pt-BR": "./locales/native/pt-BR.json",
      "pt-PT": "./locales/native/pt-PT.json",
      "en-AU": "./locales/native/en-AU.json",
      "en-CA": "./locales/native/en-CA.json"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.asugan.wiccawitchcraft",
      "usesNonExemptEncryption": false,
      "infoPlist": {
        "CFBundleLocalizations": [
          "en-US",
          "en-GB",
          "tr",
          "de-DE",
          "es-ES",
          "es-MX",
          "fr-FR",
          "fr-CA",
          "it",
          "pt-BR",
          "pt-PT",
          "en-AU",
          "en-CA"
        ]
      }
    },
    "android": {
      "permissions": [
        "INTERNET"
      ],
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.asugan.wiccawitchcraft"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-notifications",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#181611"
        }
      ],
      "expo-sqlite",
      "expo-localization"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "eas": {},
      "router": {}
    }
  }
}
